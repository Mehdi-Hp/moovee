	"private": true,
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack --hide-modules --optimize-minimize --progress --config webpack.prod.config.js",
		"build:dev": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --config webpack.dev.config.js",
		"postinstall": "link-module-alias",
		"start": "cross-env NODE_ENV=production DEBUG=app:* node ./server.js",
		"start:dev": "cross-env NODE_ENV=development DEBUG=app:* nodemon --delay 1 ./server.js",
		"start:dev:inspect": "cross-env NODE_ENV=development DEBUG=app:* ndb nodemon --delay 1 --inspect ./server.js"
	},
	"_moduleAliases": {
		"~root": ".",
		"~services": "./services",
		"~models": "./models",
		"~config": "./config",
		"~utils": "./utils",
		"~controllers": "./controllers",
		"~helpers": "./helpers",
		"~handlers": "./handlers",
		"~graphql": "./graphql",
		"~routes": "./routes"
	},
	"hyperlayout": {
		"default": {
			"entry": "tab",
			"layout": [
				"mongod",
				"npm run start:dev",
				"npm run build:dev"
			]
		}
	},
